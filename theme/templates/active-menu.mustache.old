{{#if children.length}}
	<ul {{#isFirstLevelChild}}class="primary-grouping"{{/isFirstLevelChild}}>
		{{#orderedChildren children}}
			{{^if hide}}
				{{#isGroup}}
					
					<li class="heading search-container">
						<span>{{getTitle}}</span>
						{{> active-menu.mustache}}
					</li>
					
				{{/isGroup}}
				{{^isGroup}}
					<li class="search-container{{#isActive}} active{{/isActive}} {{#children.length}}parent{{/children.length}} {{#hasActive}}expanded{{/hasActive}}">
						<a 	class="{{type}}" 
							href="{{makeHref name}}" 
							data-search="{{#if title}}{{title}}{{else}}{{{name}}}{{/if}}">
							{{getTitle}}
						</a>
						{{#hasOrIsActive}}
							{{> active-menu.mustache}}
						{{/hasOrIsActive}}
					</li>
				{{/isGroup}}
			
			{{/if}}
		{{/orderedChildren}}
	</ul>
{{/if}}